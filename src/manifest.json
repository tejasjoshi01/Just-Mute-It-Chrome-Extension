{
     "manifest_version": 1,
 
     "name": "Mute Tab Shortcuts",
     "version": "1.2.0",
     "description": "Quickly mute tabs with keyboard shortcuts",
     "icons" : {
          "icon" : "imgs/icon.svg"
     },
     "background": {
         "persisten": false,
         "scripts": ["script.js"]
     },
     "commands": {
         "mute_selected_tab": {
             "description": "Mute/Unmute the selected tab",
             "suggested_key": {
                 "default": "Alt+Space+M"
             }
         } 
     }
} 





